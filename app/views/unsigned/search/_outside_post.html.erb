    <%= stylesheet_link_tag "style", :media => "all" %>

<% @results.each do |result| %>
        <div class="row-fluid">
         <div class="span12">
          <div class="row-fluid">
            <div class="span2">
              <%= image_tag result.user.profile_image_thumb %>
            </div>
            <div class="span18">
              <%= render "/signed/feeds/feed_header", :feed => result %>
            </div>
          </div>
         </div>
        </div>

          <!-- Feed Content -->

        <%= render "/signed/feeds/feed_content", :feed => result %>
        
        <!-- for moods -->
          <br><span  class="heart_rate">
              <%= render "signed/feeds/update_heart_rate", :feed => result %>
          </span>

        <!-- Wow Pin Share -->
          <span class="span_content pin_share">
            <% if result.user_feeds.where('cool' => true).count > 0 %>
                  <img src='/img/relayimg/wow.png' />
                  <%= result.user_feeds.where('cool' => true).count %>
            <% end %>
            <% if result.user_feeds.where('favorite' => true).count > 0 %>
                   <img src='/img/relayimg/pin.png' />
                   <%= result.user_feeds.where('favorite' => true).count %>
            <% end %>
            <% if result.user_feeds.where('share' => true).count > 0 %>
                   <img src='/img/relayimg/share.png' />
                   <%= result.user_feeds.where('share' => true).count %>
            <% end %>
          </span><hr>
      <% end %>
